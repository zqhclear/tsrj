package org.tsrj.common.domain;

public class MemberCache {

	private Long memberId;
	
	/** 存管账号 **/
	private String platcust;

	/** 身份证号 **/
	private String idCard;
	
	/** 是否实名 **/
	private boolean hasRealName;
	
	/** 手机号 **/
	private String mobile;
	
	/** 头像 **/
	private String avatars;
	
	/** 是否存在密码 **/
	private boolean hasLoginPwd;
	
	/** 是否存在交易密码 **/
	private boolean hasPayPwd;
	
	/** 用户token **/
	private String token;
	
	/** 昵称 **/
	private String nickName;
	
	/** 真实名称 **/
	private String trueName;
	
	/** 通道ID, 推送通知 **/
	private String pushRegisterId;

	public Long getMemberId() {
		return memberId;
	}

	public void setMemberId(Long memberId) {
		this.memberId = memberId;
	}

	public String getPlatcust() {
		return platcust;
	}

	public void setPlatcust(String platcust) {
		this.platcust = platcust;
	}

	public String getIdCard() {
		return idCard;
	}

	public void setIdCard(String idCard) {
		this.idCard = idCard;
	}

	public String getMobile() {
		return mobile;
	}

	public void setMobile(String mobile) {
		this.mobile = mobile;
	}

	public String getAvatars() {
		return avatars;
	}

	public void setAvatars(String avatars) {
		this.avatars = avatars;
	}

	public String getToken() {
		return token;
	}

	public void setToken(String token) {
		this.token = token;
	}

	public String getNickName() {
		return nickName;
	}

	public void setNickName(String nickName) {
		this.nickName = nickName;
	}

	public String getTrueName() {
		return trueName;
	}

	public void setTrueName(String trueName) {
		this.trueName = trueName;
	}

	public String getPushRegisterId() {
		return pushRegisterId;
	}

	public void setPushRegisterId(String pushRegisterId) {
		this.pushRegisterId = pushRegisterId;
	}

	public boolean isHasRealName() {
		return hasRealName;
	}

	public void setHasRealName(boolean hasRealName) {
		this.hasRealName = hasRealName;
	}

	public boolean isHasLoginPwd() {
		return hasLoginPwd;
	}

	public void setHasLoginPwd(boolean hasLoginPwd) {
		this.hasLoginPwd = hasLoginPwd;
	}

	public boolean isHasPayPwd() {
		return hasPayPwd;
	}

	public void setHasPayPwd(boolean hasPayPwd) {
		this.hasPayPwd = hasPayPwd;
	}

	@Override
	public String toString() {
		return "MemberCache [memberId=" + memberId + ", platcust=" + platcust + ", idCard=" + idCard + ", hasRealName="
				+ hasRealName + ", mobile=" + mobile + ", avatars=" + avatars + ", hasLoginPwd=" + hasLoginPwd
				+ ", hasPayPwd=" + hasPayPwd + ", token=" + token + ", nickName=" + nickName + ", trueName=" + trueName
				+ ", pushRegisterId=" + pushRegisterId + "]";
	}
	
}
